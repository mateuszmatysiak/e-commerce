{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {},
    "db:migrate:dev": {},
    "db:push": {},
    "db:seed": {},
    "generate": {
      "dependsOn": ["^generate"],
      "cache": false
    }
  }
}
